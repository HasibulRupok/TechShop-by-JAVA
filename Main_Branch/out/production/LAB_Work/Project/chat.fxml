<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="400.0" maxWidth="700.0" prefHeight="400.0" prefWidth="700.0" style="-fx-background-color: #062A38;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project.ChatController">
   <children>
      <AnchorPane prefHeight="400.0" prefWidth="177.0" style="-fx-background-color: #001F2B;">
         <children>
            <Button fx:id="backBtn" layoutX="9.0" layoutY="210.0" mnemonicParsing="false" onAction="#oldChatPressed" prefHeight="30.0" prefWidth="160.0" style="-fx-background-color: #062A38;" text="Old Chat" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font></Button>
            <Button fx:id="exitBtn" layoutX="9.0" layoutY="285.0" mnemonicParsing="false" onAction="#exitPressed" prefHeight="30.0" prefWidth="160.0" style="-fx-background-color: #062A38;" text="EXIT" textFill="#ff6868">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font></Button>
            <Button fx:id="homeBtn" layoutX="9.0" layoutY="173.0" mnemonicParsing="false" onAction="#homePressed" prefHeight="30.0" prefWidth="160.0" style="-fx-background-color: #062A38;" text="HOME" textFill="WHITE">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font></Button>
            <ImageView fitHeight="125.0" fitWidth="167.0" layoutX="10.0" layoutY="20.0">
               <image>
                  <Image url="@images/logo2.PNG" />
               </image>
            </ImageView>
            <Button fx:id="startButton" layoutX="9.0" layoutY="247.0" mnemonicParsing="false" onAction="#deleteChatPressed" prefHeight="30.0" prefWidth="160.0" style="-fx-background-color: #062A38;" text="Delete Old Chat" textFill="#ff6868">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <TextArea fx:id="showArea" editable="false" layoutX="204.0" layoutY="48.0" prefHeight="234.0" prefWidth="469.0" promptText="Your message will show here" styleClass="jfx-text-area" stylesheets="@../CSS/white.css" />
      <TextField fx:id="inputField" layoutX="205.0" layoutY="293.0" prefHeight="36.0" prefWidth="378.0" promptText="Enter your name" style="-fx-background-color: transparent;" stylesheets="@../CSS/white.css">
         <styleClass>
            <String fx:value="jfx-button" />
            <String fx:value="white-text" />
         </styleClass></TextField>
      <Button fx:id="button" layoutX="589.0" layoutY="293.0" mnemonicParsing="false" onAction="#sendPressed" prefHeight="36.0" prefWidth="85.0" style="-fx-background-color: transparent;" styleClass="jfx-button" stylesheets="@../CSS/white.css" text="Start sms" textFill="WHITE" wrapText="true">
         <font>
            <Font size="13.0" />
         </font></Button>
   </children>
</AnchorPane>
